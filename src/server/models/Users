import { FaCruzeiroSign } from 'react-icons/fa6';

const mongoose = require('mongoose');

const userSchema = new mongoose.Schema({
    username: {
        type: String,
        required: true,
    },
    password: {
        type: String,
        required: true,
    },
    email: {
        type: String,
        required: true,
    },
    friends: {
        type: [String],
        required: false,
    },
    liked: {
        type: [String],
        required: false,
    },
    cuisinesLiked: {
        type: String,
        required: false,
    },
});

module.exports = mongoose.model('Users', userSchema);